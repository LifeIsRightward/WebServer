	<% 
		String uid = ""; 
		String pw = "";
		String email = "";
		String name = "";
		String age = "";
		String sex = "";
		uid = request.getParameter("id");
		
		if(uid != null){
			String idnum = uid;
			memberDBCP mDBCP = new memberDBCP();
			member se = mDBCP.getMember(idnum);
			
			pw = se.getPassword();
			email = se.getEmail();
			name = se.getName();
			age = se.getAge();
			sex = se.getSex();
		}
		
